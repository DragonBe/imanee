{
    "name": "imanee/imanee",
    "description": "A PHP Image Manipulation Library based on Imagemagick",
    "keywords": ["image", "php", "manipulation", "imagick", "imagemagick"],
    "homepage": "http://imanee.github.com/imanee",
    "license": "MIT",
    "authors": [
        {
            "name": "Erika Heidi",
            "email": "erika@erikaheidi.com"
        },{
            "name": "Hugo Reinaldo",
            "email": "hugo.reinaldo@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "ext-imagick": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },
    "autoload": {
        "psr-4": { "Imanee\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Imanee\\Tests\\": "tests/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
